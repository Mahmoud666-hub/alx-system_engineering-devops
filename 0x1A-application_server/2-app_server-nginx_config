server {
        listen 80;
        server_name airbnb.com;
        root /var/www/html;

        index index.html index.htm index.nginx-debian.html;

        server_name 3.90.70.233;

        location /airbnb-onepage/ {
        proxy_pass http://127.0.0.1:5000;

        }

        error_page 404 /404.html;
        location = /404.html {
            root /usr/share/nginx/html;
            internal;
        }

        location / {
            # other conf if any
        }
}